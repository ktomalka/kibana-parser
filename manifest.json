{
    "manifest_version": 3,
    "name": "KIBANA Parser",
    "version": "1.0",
    "description": "Kibana log parser",
    "permissions": ["contextMenus", "scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    }
}
